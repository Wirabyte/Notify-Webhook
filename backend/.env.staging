# Staging Environment Configuration
NODE_ENV=staging
PORT=3000

# Database Configuration
DB_TYPE=sqlite
DB_DATABASE=database-staging.sqlite
DB_SYNCHRONIZE=false
DB_LOGGING=false

# API Documentation (enabled in staging)
API_DOCS_ENABLED=true
API_DOCS_PATH=api

# CORS Configuration
CORS_ORIGINS=https://staging.yourapp.com,http://localhost:4200
CORS_CREDENTIALS=true

# Application Information
APP_NAME=Webhook Notification API (Staging)
APP_VERSION=1.0.0
APP_DESCRIPTION=API for managing webhooks and notifications to multiple platforms (Discord, Line, Telegram) - Staging Environment

# Contact Information
CONTACT_NAME=API Support
CONTACT_URL=https://github.com/Thanaphol47825/Notify-Webhook
CONTACT_EMAIL=support@example.com